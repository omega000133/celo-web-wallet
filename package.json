{
  "name": "celo-web-wallet",
  "version": "1.1.5",
  "description": "A lightweight web and desktop wallet for the Celo network",
  "keywords": [
    "Celo",
    "wallet"
  ],
  "author": "J M Rossy",
  "repository": {
    "type": "git",
    "url": "https://github.com/celo-tools/celo-web-wallet"
  },
  "homepage": "https://celowallet.app",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development webpack serve --devtool inline-cheap-source-map --mode development",
    "dev-win": "SET NODE_ENV=development && webpack serve --devtool inline-cheap-source-map --mode development",
    "build:dev": "NODE_ENV=development webpack --devtool source-map --mode development",
    "build:staging": "./scripts/build.sh -n Alfajores",
    "build:prod": "./scripts/build.sh -n Mainnet",
    "clean": "rm -rf dist dist-electron",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "test": "NODE_ENV=test jasmine --random=false",
    "test:all": "yarn test && yarn lint",
    "electron:postinstall": "electron-builder install-app-deps",
    "electron:build": "./scripts/build.sh -n Mainnet -e",
    "electron:dev": "yarn electron:build && electron ./dist/main.js --enable-logging",
    "electron:dist": "DEBUG=electron-builder electron-builder --publish never"
  },
  "dependencies": {
    "@celo-tools/celo-ethers-wrapper": "^0.0.5",
    "@emotion/react": "^11.1.5",
    "@ledgerhq/hw-app-eth": "^5.50.0",
    "@ledgerhq/hw-transport-node-hid-noevents": "^5.50.0",
    "@ledgerhq/hw-transport-u2f": "5.36.0-deprecated",
    "@ledgerhq/hw-transport-webusb": "^5.50.0",
    "@metamask/jazzicon": "https://github.com/jmrossy/jazzicon#7a8df28",
    "@reduxjs/toolkit": "^1.5.1",
    "@walletconnect/client": "2.0.0-beta.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "electron-updater": "^4.3.8",
    "ethers": "^5.1.4",
    "frappe-charts": "^1.6.1",
    "history": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-frappe-charts": "^4.0.0",
    "react-redux": "^7.2.4",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-window": "^1.8.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "stream-browserify": "^3.0.0",
    "typed-redux-saga": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/plugin-transform-strict-mode": "^7.12.13",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@emotion/babel-plugin": "^11.3.0",
    "@testing-library/react": "^11.2.6",
    "@types/jasmine": "^3.7.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-window": "^1.8.3",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-name-mapper": "^1.2.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "electron": "^11.4.5",
    "electron-builder": "^22.11.1",
    "electron-notarize": "^1.0.0",
    "eslint": "^7.25.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "jasmine": "^3.7.0",
    "jsdom": "^16.5.3",
    "node-fetch": "^2.6.1",
    "prettier": "2.2.1",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
